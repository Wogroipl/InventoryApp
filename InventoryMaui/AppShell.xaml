<?xml version="1.0" encoding="UTF-8" ?>
<Shell x:Class="InventoryMaui.AppShell"
       xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:views="clr-namespace:InventoryMaui.Views"
       xmlns:vm="clr-namespace:InventoryMaui.ViewModels"
       FlyoutBehavior="Flyout"
       Title="InventoryMaui">

    <ShellContent Title="Home"
                  ContentTemplate="{DataTemplate views:MainPage}"
                  Route="MainPage" />

    <ShellContent Title="Inventory"
                  ContentTemplate="{DataTemplate views:InventoryView}"
                  Route="InventoryView" />

</Shell>
